<?xml version="1.0" encoding="UTF-8"?><project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>de.mxro.utils</groupId>
  <artifactId>mxUtilities</artifactId>
  <name>MxUtilities</name>
  <version>0.0.1</version>
  <description>General utility classes</description>
  <url>http://www.mxro.de/</url>
  <scm>
  	<connection>scm:svn:https://mroh004:auckland10@stf-6121-dsk06.com.auckland.ac.nz/svn/MxModules/mxUtilities/trunk/</connection>
  	<developerConnection>scm:svn:https://mroh004:auckland10@stf-6121-dsk06.com.auckland.ac.nz/svn/MxModules/mxUtilities/trunk/</developerConnection>
  </scm>
  <build>
  	
  	
  	<resources>
  		<resource>
  			<targetPath>OSGI-INF</targetPath>
  			<filtering>false</filtering>
  			<directory>OSGI-INF</directory>
  		</resource>
  	</resources>
  	
  	
  	<plugins>
      
      <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-release-plugin</artifactId>
                <configuration>
                        <tagBase>svn:https://mroh004:auckland10@stf-6121-dsk06.com.auckland.ac.nz/svn/MxModules/mxUtilities/tags</tagBase>
                        <scmCommentPrefix>release: </scmCommentPrefix>
                </configuration>
        </plugin>
      
      <plugin>
      	<groupId>org.apache.maven.plugins</groupId>
      	<artifactId>maven-jar-plugin</artifactId>
      	<configuration>
      		<archive>
      			<manifestFile>META-INF/MANIFEST.MF</manifestFile>
      		</archive>
        </configuration>
      </plugin> 
         
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.6</source>
          <target>1.6</target>
        </configuration>
      </plugin>
    </plugins>
    
  </build>
  
  <distributionManagement>
        <repository>
            <id>releases</id>
            <name>Internal Releases</name>
            <url>http://STF-6121-DSK06.com.auckland.ac.nz:8080/nexus/content/repositories/releases</url>
        </repository>
        <snapshotRepository>
            <id>snapshots</id>
            <name>Internal Snapshots</name>
            <url>http://STF-6121-DSK06.com.auckland.ac.nz:8080/nexus/content/repositories/snapshots</url>
        </snapshotRepository>
    </distributionManagement>
    
</project>